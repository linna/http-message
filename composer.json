{
    "name": "linna/http-message",
    "type": "library",
    "description": "Linna PSR-7 implementation",
    "keywords": ["psr7", "psr", "http", "http-message", "php"],
    "homepage": "https://github.com/linna/http-message",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Rapetti",
            "email": "sebastian.rapetti@tim.it"
        }
    ],
    "support": {
        "issues": "https://github.com/linna/http-message/issues"
    },
    "require": {
        "php": ">=7.2",
        "s3b4stian/http-message": "dev-master",
        "linna/typed-array" :  "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0"
    },
    "provide": {
        "psr/http-message-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "Linna\\": "src/Linna/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linna\\":  "tests/Linna/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
